FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../setUp"

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    east
    {
        type            inletOutlet;
        inletValue      uniform 0.01;
        value           $internalField;
		
    }
    north
    {
        type            slip;
		
    }
    south
    {
        type            slip;
		
    }
    west
    {
        type            atmBoundaryLayerInletEpsilon;
        Uref            $Uref;
        Zref            $Zref;
        zDir            (0 0 1);
        flowDir         (1 0 0);
        z0              uniform $z0;
        zGround         uniform 0.75;
    }

    minZ
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.4;
        E               9.8;
        value           $internalField;
    }
    maxZ
    {
        type            slip;
    }
}


// ************************************************************************* //
