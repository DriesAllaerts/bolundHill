FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    east
    {
        type            totalPressure;
        value           uniform 0;
        gamma           1;
        p0              uniform 0;
	}
    north
    {
        type            totalPressure;
        value           uniform 0;
        gamma           1;
        p0              uniform 0;
	}
    south
    {
        type            totalPressure;
        value           uniform 0;
        gamma           1;
        p0              uniform 0;
	}
    west
    {
        type            zeroGradient;
		
    }

    minZ
    {
        type            zeroGradient;
    }
    maxZ
    {
        type            fixedValue;
        value           uniform 0;
    }
}


