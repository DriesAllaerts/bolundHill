FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    north
    {
        type            fixedValue;
        value           uniform 1;
    }

    west
    {
        type            zeroGradient;
    }

    east
    {
        type            zeroGradient;
    }

    south
    {
        type            zeroGradient;
    }

    minZ
    {
        type            zeroGradient;
    }

    maxZ
    {
        type            zeroGradient;
    }
}

