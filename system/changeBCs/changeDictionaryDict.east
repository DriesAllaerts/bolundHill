FoamFile
{
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../../setUp";

dictionaryReplacement
{
    U
    {
        boundaryField
        {
            north
            {
                type            pressureInletOutletVelocity;
        		value           uniform (0 0 0);
            }
            east
            {
                type            atmBoundaryLayerInletVelocity;
                Uref            $Uref;
                Zref            $Zref;
                zDir            (0 0 1);
                flowDir         (-1 0 0);
                z0              uniform $z0;
                zGround         uniform 0.75;
            }
            south
            {
                type            pressureInletOutletVelocity;
        		value           uniform (0 0 0);
            }
            west
            {
                type            pressureInletOutletVelocity;
        		value           uniform (0 0 0);
            }
        }
    }

    epsilon
    {
        boundaryField
        {
            north
            {
                type            slip;
            }
            east
            {
                type            atmBoundaryLayerInletEpsilon;
                Uref            $Uref;
                Zref            $Zref;
                zDir            (0 0 1);
                flowDir         (-1 0 0);
                z0              uniform $z0;
                zGround         uniform 0.75;
            }
            south
            {
                type            slip;
            }
            west
            {
                type            inletOutlet;
                inletValue      uniform 0.01;
                value           uniform 0.01;
            }
        }
    }

    k
    {
        boundaryField
        {
            north
            {
                type            slip;
            }
            east
            {
                type            atmBoundaryLayerInletK;
                Uref            $Uref;
                Zref            $Zref;
                zDir            (0 0 1);
                flowDir         (-1 0 0);
                z0              uniform $z0;
                zGround         uniform 0.75;
            }
            south
            {
                type            slip;
            }
            west
            {
                type            inletOutlet;
                inletValue      uniform 1.3;
                value           uniform 1.3;
            }
        }
    }

    nut
    {
        boundaryField
        {
            north
            {
                type         slip;
            }
            east
            {
                type            calculated;
                value           uniform 0;
            }
            south
            {
                type            slip;
            }
            west
            {
                type            calculated;
                value           uniform 0;
            }
        }
    }

    p
    {
        boundaryField
        {
            north
            {
                type            totalPressure;
                value           uniform 0;
                gamma           1;
                p0              uniform 0;
        	}
            east
            {
                type            zeroGradient;
        	}
            south
            {
                type            totalPressure;
                value           uniform 0;
                gamma           1;
                p0              uniform 0;
        	}
            west
            {
                type            totalPressure;
                value           uniform 0;
                gamma           1;
                p0              uniform 0;
            }
        }
    }
}
