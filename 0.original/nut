#include        "../setUp"

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    north
    {
        type            slip;
    }
    south
    {
        type            slip;
    }
    west
    {
        type            calculated;
        value           uniform 0;
    }
    east
    {
        type            calculated;
        value           uniform 0;
    }
    minZ
    {
        type            nutkAtmRoughWallFunction;
        z0              uniform $z0;
        value           uniform 0;
    }
    maxZ
    {
        type            slip;
    }
}


